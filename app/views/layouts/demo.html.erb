<!DOCTYPE html>
<html>
<head>
  <title>Processes</title>
  <%= stylesheet_link_tag    "demo", media: "all" %>
  <%= javascript_include_tag "process" %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="container">
  <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, id: "flash_#{name}" %>
  <% end %>
  <%= yield %>
  <div class="clear"></div>
</div>
</body>
</html>
